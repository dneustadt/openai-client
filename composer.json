{
    "name": "dneustadt/openai-client",
    "description": "Client for OpenAI API",
    "keywords": [
        "openapitools",
        "openapi-generator",
        "openapi",
        "php",
        "sdk",
        "rest",
        "api"
    ],
    "homepage": "https://neustadt.dev",
    "license": "MIT",
    "authors": [
        {
            "name": "David Neustadt",
            "homepage": "https://neustadt.dev"
        }
    ],
    "require": {
        "php": ">=7.2",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^6.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0 || ^9.0",
        "friendsofphp/php-cs-fixer": "^2.12",
        "symfony/dotenv": "^6.0"
    },
    "autoload": {
        "psr-4": { "OpenAI\\Client\\" : "lib/" }
    },
    "autoload-dev": {
        "psr-4": { "OpenAI\\Client\\Test\\" : "test/" }
    }
}
